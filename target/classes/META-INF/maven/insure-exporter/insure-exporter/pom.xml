<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>insure-exporter</groupId>
	<artifactId>insure-exporter</artifactId>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.6.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>insure.tools</groupId>
			<artifactId>insure-feldsteuerung-persistence</artifactId>
		</dependency>
		<!-- https://mvnrepository.com/artifact/commons-lang/commons-lang -->
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>2.6</version><!--$NO-MVN-MAN-VER$ -->
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-jdk14</artifactId>
			<version>1.7.14</version>
		</dependency>
		<dependency>
			<groupId>net.sf.ehcache</groupId>
			<artifactId>ehcache</artifactId>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>insure.infoservice</groupId>
			<artifactId>insure-infoservice-modell</artifactId>
		</dependency>
		<dependency>
			<groupId>de.adesso.ais.infoservice-reference</groupId>
			<artifactId>infoservice-reference-modell</artifactId>
		</dependency>
		<dependency>
			<groupId>insure.domain</groupId>
			<artifactId>insure.domain.modell</artifactId>
		</dependency>
		<dependency>
			<groupId>de.adesso.ais.domain-reference</groupId>
			<artifactId>de.adesso.ais.domainreference.modell</artifactId>
		</dependency>
	</dependencies>
	<parent>
		<groupId>insure-exporter-parent</groupId>
		<artifactId>insure-exporter-parent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
</project>